@model IEnumerable<Dziennik.Models.Nieobecnosc>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Lekcja.Nauczyciel)
        </th>
        <th>
            Godzina
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lekcja)
        </th>
        <th>
            Data
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Lekcja.Nauczyciel.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lekcja.godzina)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lekcja.Przedmiot.nazwa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>


    </tr>
    }

</table>
