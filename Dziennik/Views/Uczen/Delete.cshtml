@model Dziennik.Models.Uczen

@{
    ViewBag.Title = "Delete";
}

<h2>Usuwanie</h2>

<h3>Czy jesteś pewien, że chcesz usunąc danego użytkownika?</h3>
<div>
    <h4>Uczen</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Imię
        </dt>

        <dd>
            @Html.DisplayFor(model => model.imie)
        </dd>

        <dt>
            Nazwisko
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nazwisko)
        </dd>

        <dt>
           Rozdzic
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Rodzic.imie)
            @Html.DisplayFor(model => model.Rodzic.nazwisko)
        </dd>

        <dt>
            Klasy
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Klasa.level)
            @Html.DisplayFor(model => model.Klasa.nazwa)
        </dd>

        <dt>
            Login
        </dt>

        <dd>
            @Html.DisplayFor(model => model.login)
        </dd>

        <dt>
            Hasło
        </dt>

        <dd>
            @Html.DisplayFor(model => model.haslo)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Usuń" class="btn btn-default" /> |
            @Html.ActionLink("Powrót", "Index")
        </div>
    }
</div>
