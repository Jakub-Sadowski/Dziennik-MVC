@model IEnumerable<Dziennik.Models.Donos>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nauczyciel.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.wiadomosc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.data_pytania)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nauczyciel.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.wiadomosc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.data_pytania)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
